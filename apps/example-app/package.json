  "devDependencies": {
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.25.0",
    "@toolchain/eslint-config": "workspace:*",
    "svelte": "4.2.0",
    "svelte-check": "3.5.2",
    "tslib": "2.6.2",
    "vite": "4.4.9"
  }
}
